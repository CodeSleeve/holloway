{
    "name": "codesleeve/holloway",
    "description": "Holloway: A datamapper built on top of the Illuminate Database package.",
    "keywords": ["datamapper", "laravel", "illuminate", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Bennett",
            "email": "tandrewbennett@hotmail.com"
        }, {
            "name": "Kelt Dockins",
            "email": "ktd_@hotmail.com"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "illuminate/database": ">= 8.0",
        "illuminate/pagination": ">= 8.0",
        "illuminate/events": "^8.0",
        "laravel/legacy-factories": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "CodeSleeve\\Holloway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeSleeve\\Holloway\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "doctrine/instantiator": "~1.4.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "minium-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "CodeSleeve\\Holloway\\HollowayServiceProvider"
            ]
        }
    }
}
