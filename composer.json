{
    "name": "codesleeve/holloway",
    "description": "Holloway: A datamapper built on top of Illuminate\\Database.",
    "keywords": ["datamapper, laravel, Illuminate, database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Bennett",
            "email": "tandrewbennett@hotmail.com"
        }, {
            "name": "Kelt Dockins",
            "email": "ktd_@hotmail.com"
        }
    ],
    "require": {
        "illuminate/database": ">= 5.8",
        "illuminate/pagination": ">= 5.8",
        "php": ">= 7.4"
    },
    "autoload": {
        "psr-4": {
            "CodeSleeve\\Holloway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeSleeve\\Holloway\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {"type": "path", "url": "./psr"}
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "illuminate/events": "^5.5",
        "doctrine/instantiator": "~1.0.3"
    },
    "minium-stability": "stable",
    "laravel": {
        "providers": [
            "CodeSleeve\\Holloway\\HollowayServiceProvider"
        ]
    }
}
