{
    "name": "codesleeve/holloway",
    "description": "Holloway DataMapper.",
    "keywords": ["datamapper"],
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Bennett",
            "email": "tandrewbennett@hotmail.com"
        }, {
            "name": "Kelt Dockins",
            "email": "ktd_@hotmail.com"
        }
    ],
    "require": {
        "illuminate/database": ">= 5.8",
        "illuminate/pagination": ">= 5.8",
        "php": ">= 7.3"
    },
    "autoload": {
        "psr-4": {
            "Holloway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {"type": "path", "url": "./psr"}
    ],
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "illuminate/events": "^5.5"
    },
    "minium-stability": "stable"
}
